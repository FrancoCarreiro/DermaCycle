+                        .resizable()
+                        .contextMenu {
+                            Button {
+                            } label : {
+                                Text(item.Whatsapp!)
+                            }
+                        }
+                    Image(systemName: "paperplane.circle.fill")
+                        .resizable()
+                        .contextMenu {
+                            Button {
+                            } label : {
+                                Text(item.Direccion!)
+                            }
