+                            Button {
+                            } label: {
+                                Text(item.Facbook!) }
+                            Button {
+                            } label: {
+                                Text(item.Instagram!)
+                            }
+                            Button {
+                            } label: {
+                                Text(item.Twitter!)
+                            }
+                        }
+                    Spacer()
+                } .padding()
+                
+            } .task {
+                await datosContacto.fetchContacto()
+            }
+            
+        }
+    }
